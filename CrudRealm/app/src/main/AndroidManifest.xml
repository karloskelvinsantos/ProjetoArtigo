<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.karloskelvin.crudrealm">

    <application
        android:name=".util.CustomApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.AdicionarContatoActivity"
            android:label="@string/btnAdd" />
        <activity
            android:name=".activitys.ListarContatosActivity"
            android:label="@string/listarContatos" />
        <activity android:name=".activitys.AdicionarCargaDadosActivity"></activity>
    </application>

</manifest>
